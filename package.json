{
  "name": "express-fcm-to-apns",
  "version": "0.0.1",
  "description": "Express middleware that intercepts FCM token storage calls and listens for messages, converting them to APNS",
  "keywords": [
    "apns",
    "express",
    "fcm"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "author": "Kris Pomphrey <kris@tkf.network>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -cw",
    "lint": "eslint 'src/**/*.{json,ts,tsx}'",
    "prettier": "prettier 'src/**/*.{ts,tsx,json}'",
    "prettier:check": "yarn prettier --check",
    "prettier:write": "yarn prettier --write",
    "test": "jest",
    "test:ci": "cross-env CI=true jest --coverage",
    "test:coverage": "jest --coverage",
    "test:coverage:badge": "make-coverage-badge",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "express-http-proxy": "^1.6.2",
    "object-hash": "^2.2.0",
    "push-receiver": "^2.1.1",
    "ramda": "^0.27.1",
    "rxjs": "^7.3.0"
  },
  "peerDependencies": {
    "express": "^4.17.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/express-http-proxy": "^1.6.2",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.0",
    "@types/object-hash": "^2.1.1",
    "@types/ramda": "^0.27.44",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^2.3.2",
    "rollup": "^2.55.1",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  }
}
